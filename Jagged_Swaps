#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
string solve(ll n)
{
    vector<int>v(n);
    for(int i =0;i<n;i++)
        cin>>v[i];
    
    if(v[0]!=1)
        return "NO";
    
    return"YES";
}
int main()
{
    ll T;
    cin>>T;

    while(T--)
    {
        ll n;
        cin>>n;

        string ans = solve(n);
        cout<<ans<<endl;
    }

}
